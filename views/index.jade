extends layout

block content

  div(ng-controller="EggsController as eggsCtrl")
    div(â€¨ng-repeat="egg in eggsCtrl.eggs")
      table.table.table-bordered.table-condensed.center-block(style="width:80%; text-align ")
        tr
          th.col-md-3 Field
          th.col-md-4 Form Data
          th.col-md-4 Serial Data
        tr
          td Sensor Type
          td#sensor-type(colspan='2',style={"text-align": "center"}) {{egg.serial.sensorType}}
        tr
          td MAC Address
          td#form-data-mac-address {{egg.form.macAddress}}
          td#serial-data-mac-address {{egg.serial.macAddress.value}}
        tr
          td OpenSensors Username
          td#form-data-open-sensors-username {{egg.form.openSensorsUsername}}
          td#serial-data-open-sensors-username {{egg.serial.openSensorsUsername.value}}
        tr
          td OpenSensors Password
          td#form-data-open-sensors-password {{egg.form.openSensorsPassword}}
          td#serial-data-open-sensors-password {{egg.serial.openSensorsUsername.value}}
        tr
          td Shipped Firmware
          td#form-data-shipped-firmware {{egg.form.shippedFirmwareVersion}}
          td#serial-data-shipped-firmware {{egg.serial.shippedFirmwareVersion.value}}
        tr(ng-show="egg.serial.sensorType == 'NO2 / CO'")
          td CO S/N
          td#form-data-co-serial-number {{egg.form.coSerialNumber}}
          td#serial-data-co-serial-number {{egg.serial.coSerialNumber.value}}
        tr(ng-show="egg.serial.sensorType == 'NO2 / CO'")
          td CO Date Code
          td#form-data-co-date-code {{egg.form.coDateCode}}
          td#serial-data-co-date-code {{egg.serial.coDateCode.value}}
        tr(ng-show="egg.serial.sensorType == 'NO2 / CO'")
          td CO Sensitivity
          td#form-data-co-sensitivity {{egg.form.coSensitivity}}
          td#serial-data-co-sensitivity {{egg.serial.coSensitivity.value}}
        tr(ng-show="egg.serial.sensorType == 'NO2 / CO'")
          td CO Offset
          td#form-data-co-offset {{egg.form.coOffset}}
          td#serial-data-co-offset {{egg.serial.coOffset.value}}
        tr(ng-show="egg.serial.sensorType == 'NO2 / CO'")
